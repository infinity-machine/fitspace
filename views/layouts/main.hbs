            <li> <a class="waves-effect waves-light orange lighten-2 btn blue-text text-darken-2" href="/auth/logout"><i class="material-icons right"></i>Log Out</a></li>
          </ul>


      <div class="row col s12" id="Dashboard">
        <div class="container">
            <div class="col s12">
                
            <div class="row">
                <div class="col s12 ">
